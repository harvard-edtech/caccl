{
  "name": "caccl",
  "version": "2.0.0-beta.19",
  "description": "Canvas App Complete Connection Library: an all-in-one library for connecting your app to Canvas, handling lti, access tokens, and api.",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project ./tsconfigClient.json && tsc --project ./tsconfigServer.json && rimraf types/ && copyfiles -f node_modules/caccl-api/lib/types/*.d.ts node_modules/caccl-lti/lib/types/*.d.ts server/shared/types/*.d.ts types/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harvard-edtech/caccl.git"
  },
  "keywords": [
    "canvas",
    "instructure",
    "lms",
    "api",
    "authorization",
    "lti",
    "oauth",
    "oauth2",
    "app",
    "access token",
    "education",
    "edtech",
    "harvard",
    "dce"
  ],
  "author": "Gabe Abrams <gabeabrams@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/harvard-edtech/caccl/issues"
  },
  "homepage": "https://github.com/harvard-edtech/caccl#readme",
  "dependencies": {
    "caccl-api": "2.0.0-beta.17",
    "caccl-api-forwarder": "2.0.0-beta.15",
    "caccl-authorizer": "2.0.0-beta.15",
    "caccl-error": "2.0.0-beta.15",
    "caccl-grade-passback": "2.0.0-beta.15",
    "caccl-lti": "2.0.0-beta.16",
    "caccl-memory-store": "^2.0.0-beta.15",
    "caccl-send-request": "2.0.0-beta.15",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "memorystore": "^1.6.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  }
}
